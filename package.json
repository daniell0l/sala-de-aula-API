{
  "name": "sala-de-aula",
  "version": "1.0.0",
  "description": "Serverless google-nodejs-typescript template",
  "main": "serverless.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "sls deploy",
    "dev": "nodemon --exec ts-node src/functions/server.ts",
    "migration:generate": "typeorm-ts-node-commonjs -d ./src/functions/data-source.ts migration:generate ./src/functions/database/migration/default",
    "migration:run": "typeorm-ts-node-commonjs -d ./src/functions/data-source.ts migration:run"
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.9"
  },
  "devDependencies": {
    "@serverless/typescript": "^2.23.0",
    "express": "^4.18.1",
    "serverless": "^2.23.0",
    "serverless-google-cloudfunctions": "^4.0.0",
    "serverless-webpack": "^5.3.5",
    "ts-loader": "^8.0.15",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "webpack": "^5.20.2",
    "webpack-node-externals": "^2.5.2",
    "@types/express": "^4.17.13",
    "@types/node": "^18.7.14",
    "nodemon": "^2.0.19",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  },
  "author": "Daneil Medrado",
  "license": "MIT"
}