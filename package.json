{
  "name": "sala-de-aula",
  "version": "1.0.0",
  "description": "Serverless google-nodejs-typescript template",
  "main": "serverless.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "sls deploy",
    "dev": "nodemon --exec ts-node src/functions/server.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:migrate:dev": "set NODE_ENV=dev && ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run",
    "migration:generate": "typeorm-ts-node-commonjs -d ./src/functions/data-source.ts migration:generate ./src/functions/database/migration/default",
    "migration:run": "typeorm-ts-node-commonjs -d ./src/functions/data-source.ts migration:run"
  },
  "engines": {
    "node": ">=16"
  },
